/* pages/coupon/coupon.wxss */
.page_coupon{
  width: 750rpx;
  min-height: 100vh;
}
.coupon-list{
  width: 750rpx;
  padding: 35rpx;
  box-sizing: border-box;
}
.coupon{
  width: 680rpx;
  height: 220rpx;
  color: #fff;
  font-size: 30rpx;
  background-size: cover;
  position: relative;
}
.coupon+.coupon{
  margin-top: 30rpx;
}
.coupon.primary{
  background-image: url(data:image/png;base64,);
}
.coupon.default{
  background-image: url(data:image/png;base64,);
}
.coupon .coupon-info, .coupon .coupon-action{
  height: 186rpx;
  position: absolute;
  top: 17rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  box-sizing: border-box;
}
.coupon .coupon-info{
  width: 440rpx;
  left: 20rpx;
  padding: 28rpx 20rpx 20rpx 0;
}
.coupon .coupon-info .detail{
  width: 400rpx;
  height: 100rpx;
  margin-bottom: 14rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
}
.coupon .coupon-info .detail .money{
  font-size: 80rpx;
  display: inline-block;
}
.coupon .coupon-info .detail .condition{
  width: 170rpx;
  text-align: right;
  display: inline-block;
}
.coupon .coupon-info .validity{
  width: 400rpx;
  color: rgb(299,1,1);
  background-color: #fff;
  font-size: 24rpx;
  font-weight: bold;
  text-align: center;
  padding: 0 20rpx;
  box-sizing: border-box;
  border-radius: 30rpx;
}
.coupon.default .coupon-info .validity{
  color: rgb(153,153,153);
}
.coupon .coupon-action{
  width: 60rpx;
  right: 30rpx;
  font-size: 32rpx;
  line-height: 42rpx;
  text-align: center;
}