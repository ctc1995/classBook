/* pages/search/search.wxss */
.page_search{
    padding: 0 36rpx;
}
/* 官方书单 */
.mod_book_list .list{
    color: #999;
    font-size: 26rpx;
    display: flex;
    flex-wrap: wrap;
}
.mod_book_list .item{
    width: 211rpx;
    padding: 10rpx 0;
    margin-bottom: 30rpx;
    border-radius: 20rpx;
    text-align: center;
    background-color: #f3f3f3;
}
.mod_book_list .item:nth-child(3n+2), .mod_book_list .item:nth-child(3n+3){
  margin-left: 22rpx;
}
